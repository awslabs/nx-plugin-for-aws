  "nx": {
    "targets": {
      "update-versions": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx -y npm-check-updates@19.0.0 --configFileName .ncurc.cjs",
            "pnpm i --lockfile-only",
            "pnpm i",
            "tsx ./scripts/update-versions.ts",
            "nx reset",
            "nx test nx-plugin -u",
            "nx run-many --target lint --fix",
            "nx generate-3p-license nx-plugin"
          ],
          "parallel": false
        }
